<div id="<%= dom_id post %>">
  <p>
    <strong>User:</strong>
    <%= post.user.username%>
  </p>

  <p>
    <strong>Label:</strong>
    <%= post.label %>
  </p>
  
  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>create at:</strong>
    <%= post.created_at %>
  </p>

  <p>
    <strong>last update:</strong>
    <%= post.updated_at %>
  </p>

</div>
