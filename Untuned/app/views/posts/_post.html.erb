<div id="<%= dom_id post %>">
  <p>
    <strong>User:</strong>
    <%= post.user.username %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Like:</strong>
    <%= post.like %>
  </p>

  <p>
    <strong>Dislike:</strong>
    <%= post.dislike %>
  </p>

  <p>
    <strong>Label:</strong>
    <%= post.label %>
  </p>

</div>
